/* pages/cover/main.wxss */
.camera{
  top: 0;
  width:100%;
  height:100%;
  position: fixed;
}

.camerabgImage-view{
  height: 100%;
  width: 100%;
  position:absolute;
}
.bgImage{
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: absolute;
}
.bgImage-passport{
  top: 25%;
  width: 100%;
  height: 50%;
  position: absolute;
}

.bgFlip{
  top:0;
  left:0;
  margin-top: 5%;
  margin-left: 80%;
  width: 10%;
  height: 5%;
  position:fixed;
}

.cover{
  top:85%;
  margin-left: 27%;
  position: fixed;
  background-color: rgb(255, 123, 0);
  height: 80rpx;
  text-align: center;
  width: 46%;
  border-radius: 50rpx;
  padding-top: 30rpx;
  padding-left: 15rpx;
  padding-right: 15rpx;
  color: white;
}

.canvas{
  top: 100%;
  width: 100%;
  height: 100%;
  position: fixed;
}